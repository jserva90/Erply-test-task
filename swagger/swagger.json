{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "Swagger API"
    },
    "paths": {
      "/admin/main": {
        "get": {
          "summary": "Get the main page",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "summary": "Method not supported",
          "responses": {
            "405": {
              "description": "Method Not Allowed"
            }
          }
        }
      },
      "/": {
        "get": {
          "summary": "Get the home page",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "summary": "Create a new session",
          "parameters": [
            {
              "name": "clientCode",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Client code"
            },
            {
              "name": "username",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Username"
            },
            {
              "name": "password",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Password"
            }
          ],
          "responses": {
            "302": {
              "description": "Found"
            },
            "404": {
              "description": "Not Found"
            },
            "405": {
              "description": "Method Not Allowed"
            }
          }
        }
      },
      "/admin/savecustomer": {
        "get": {
          "summary": "Get the save customer page",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "summary": "Save a customer",
          "parameters": [
            {
              "name": "fullName",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Full name"
            },
            {
              "name": "email",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Email"
            },
            {
              "name": "phone",
              "in": "formData",
              "type": "string",
              "required": true,
              "description": "Phone number"
            }
          ],
          "responses": {
            "302": {
              "description": "Found"
            },
            "404": {
              "description": "Not Found"
            },
            "405": {
              "description": "Method Not Allowed"
            }
          }
        }
      },
      "/admin/success": {
        "get": {
          "summary": "Get the success page",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        },
        "post": {
          "summary": "Method not supported",
          "responses": {
            "405": {
              "description": "Method Not Allowed"
            }
          }
        }
      },
      "/admin/logout": {
        "get": {
          "summary": "Logout the current session",
          "responses": {
            "302": {
              "description": "Found"
            }
          }
        }
      },
      "/admin/customers": {
        "get": {
          "summary": "Fetch customers",
          "responses": {
            "200": {
              "description": "OK"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/admin/error": {
        "get": {
          "summary": "Get the error page",
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    }
  }
  